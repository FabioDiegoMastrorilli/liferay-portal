{
	"frontend-taglib-clay": {
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/frontend-taglib-clay/$1"
		},
		"file-globs": "**/*.js,**/*.soy.js",
		"input": "build/node/packageRunBuild/resources",
		"output": "build/node/packageRunBuild/resources/bridge/frontend-taglib-clay",
		"src-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "frontend-taglib-clay/$1"
		}
	},
	"metal": {
		"dest-file-mapper": {
			"from": "(.*)\\$(.*)@.*/lib/(.*)",
			"to": "$2/src/$3"
		},
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/$1"
		},
		"file-globs": "**/lib/**/*.js,!**/bridge",
		"input": "build/node/packageRunBuild/resources/node_modules",
		"output": "build/node/packageRunBuild/resources/bridge",
		"src-mod-name-mapper": {
			"from": "(.*)@[^/]*(.*)\\.js$",
			"to": "$1$2"
		}
	}
}